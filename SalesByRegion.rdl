  <DataSets>
    <DataSet Name="SalesByRegionDS">
      <Query>
        <DataSourceName>ClientWarehouseDataSource</DataSourceName>
        <CommandText>SELECT
    dc.Region,
    SUM(fs.TotalSaleAmount) AS TotalSales,
    COUNT(*) AS TotalTransactions
FROM dbo.FactSales fs
JOIN dbo.DimCustomer dc
  ON fs.CustomerKey = dc.CustomerKey
GROUP BY dc.Region
ORDER BY dc.Region;</CommandText>
      </Query>
      <Fields>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalTransactions">
          <DataField>TotalTransactions</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
